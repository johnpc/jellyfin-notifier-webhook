{
  "name": "jellyfin-message",
  "author": "",
  "license": "ISC",
  "description": "",
  "version": "0.1.0",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "clean": "rm -rf dist; rm -rf node_modules; rm -rf build/* && rm -rf build",
    "build": "prettier --write \"**/*\" && tsc",
    "watch": "tsc -w",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "prepublishOnly": "npm run build && npm run test && npm run copy:config",
    "test": "jest --coverage",
    "posttest": "generate-coverage-data --language typescript",
    "pretest": "npm run lint",
    "test:watch": "jest --coverage --watch",
    "copy:config": "copysrc",
    "prepare": "husky"
  },
  "main": "./dist/index.js",
  "exports": "./dist/index.js",
  "files": [
    "dist/",
    "!**/__tests__/**"
  ],
  "devDependencies": {
    "@aws-amplify/backend": "^1.14.1",
    "@aws-amplify/backend-cli": "^1.4.11",
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@tsconfig/node18": "^18.2.4",
    "@types/jest": "^29.5.12",
    "@types/uuid": "^10.0.0",
    "aws-cdk": "^2.1002.0",
    "aws-cdk-lib": "^2.181.1",
    "babel-jest": "^29.7.0",
    "constructs": "^10.4.2",
    "esbuild": "^0.25.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.4.0",
    "jest": "^29.7.0",
    "jest-mock": "^29.7.0",
    "lint-staged": "^15.2.10",
    "node-fetch": "^3.3.2",
    "prettier": "3.3.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^7.13.0"
  },
  "dependencies": {
    "@jellyfin/sdk": "^0.11.0",
    "aws-amplify": "^6.13.2",
    "dotenv": "^16.4.7"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --cache --fix"
    ],
    "*.json": "prettier --write",
    "*.yml": "prettier --write",
    "*.md": "prettier --write"
  }
}
